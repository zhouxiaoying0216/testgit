<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!-- <suite> 套件提供多条 <test> 用例数据 -->
<!--<suite name="test suite" verbose="1" parallel="methods" thread-count="4">-->
<suite name="查找用户">

    <test name="findUser_test1">
        <parameter name="age" value="18"/>
        <parameter name="id" value="608"/>
        <parameter name="isDelete" value="0"/>
        <parameter name="password" value="123456"/>
        <parameter name="permission" value="1"/>
        <parameter name="sex" value="0"/>
        <parameter name="userName" value="敦小"/>
        <classes>
            <!-- 执行哪个类 -->
            <class name="com.apis.FindUserJsonToJsonString">
                <!-- 执行类中的哪个方法 -->
                <methods>
                    <include name="finduserjsontojsonstring"></include>
                </methods>
            </class>
        </classes>
    </test>

</suite>